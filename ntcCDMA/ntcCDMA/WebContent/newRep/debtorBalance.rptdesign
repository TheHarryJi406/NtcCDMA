            <xml-property name="queryText"><![CDATA[select e.cc_cd,(select cc_center from v_cc_center where cc_code=e.cc_cd) cc_description,
(select acc_cen_code from v_cc_center where cc_code=e.cc_cd) acc_cen_code,
(select ac_center from v_cc_center where cc_code=e.cc_cd) ac_description,
(select region_code from v_cc_center where cc_code=e.cc_cd) region_code,
(select region from v_cc_center where cc_code=e.cc_cd) region_description,
e.ACCT_ID,e.SRV_NO, a.firstname||' '||a.middlename||' '||a.lastname cust_name,
 a.billgroupid,nvl(sum(DR_AMT),0)-nvl(sum(CR_AMT),0) as Balance 
       FROM lcc_ledger_dat e, u6test.cc_account a
        where MAIN_TRANS_TYPE = 'DEB'
        AND e.acct_id=a.accountno
        AND  acct_id BETWEEN NVL(?,acct_id) AND NVL(?,acct_id)
        AND trans_dt BETWEEN cmmn1.to_ad(?) AND cmmn1.to_ad(?)
        AND cc_cd in(SELECT cc_code 
                     FROM v_CC_CENTER WHERE cc_code=NVL(?,cc_code) 
                     AND acc_cen_code=NVL(?,acc_cen_code)
                     AND region_code=NVL(?,region_code))
                      group by ACCT_ID,SRV_NO, a.firstname||' '||a.middlename||' '||a.lastname,  a.billgroupid, e.cc_cd 
         order by e.cc_cd,ACCT_ID,SRV_NO]]></xml-property>
